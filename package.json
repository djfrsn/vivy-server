    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "nock": "^3.6.0",
    "phantomjs-prebuilt": "^2.1.4",
    "react-addons-test-utils": "^0.14.3",
    "redux-mock-store": "0.0.3",
    "shell-executor": "^0.3.2"
  },
  "engines": {
    "node": "5.x"
  },
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.7",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^6.3.19",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.14.2",
    "classnames": "^2.2.1",
    "connect-mongo": "^1.1.0",
    "css-loader": "^0.23.0",
    "deep-equal": "^1.0.1",
    "es6-promise": "^3.0.2",
    "eslint": "^0.22.1",
    "eslint-loader": "^0.12.0",
    "eslint-plugin-react": "^2.7.1",
    "express": "^4.13.3",
    "express-flash": "0.0.2",
    "express-session": "^1.12.1",
