{
  "name": "vivy-server",
  "version": "0.0.1",
  "description": "JS server built using Express 4 with Webpack, React, ImmutableJS",
  "repository": "https://github.com/djfrsn/vivy-server",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public",
    "start": "NODE_ENV=production node server/index.js",
    "dev": "NODE_ENV=development shell-exec 'node server/index.js' 'npm run test:watch'",
    "debug": "node-debug server/index.js",
    "build:webpack": "NODE_ENV=production webpack --progress --colors --config ./webpack/webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test": "karma start",
    "test:watch": "npm test -- --watch --no-single-run",
    "postinstall": "npm run build"
  },
  "author": "Dennis Jefferson",
  "license": "MIT",
  "devDependencies": {
    "expect": "^1.13.4",
    "karma": "^0.13.15",
    "karma-jsdom-launcher": "^1.0.1",
